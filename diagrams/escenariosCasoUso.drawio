        <mxCell id="-zvzukbL2mYfY_jgGDVD-52" value="Flujo alternativo (Error)" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;" vertex="1" parent="1">
